<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<HBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.inholland.nl.wimmusic.controller.MainController"
      prefHeight="536.0" prefWidth="857.0" spacing="10" style="-fx-background-color: #d9d9d9;" >

<VBox prefHeight="536.0" prefWidth="181.0" spacing="10" style="-fx-padding: 20px; -fx-background-color: #420400;">
    <Button fx:id="dashboardButton" onAction="#navigateToDashboard" prefHeight="31.0" prefWidth="139.0" style="-fx-background-color: #520601;" text="Dashboard" textFill="WHITE">
        <font>
            <Font size="15.0" />
        </font>
    </Button>
    <Button fx:id="createOrderButton" onAction="#navigateToCreateOrder" prefHeight="31.0" prefWidth="139.0" style="-fx-background-color: #520601;" text="Create Order" textFill="WHITE">
        <font>
            <Font size="15.0" />
        </font>
    </Button>
    <Button fx:id="productInventoryButton" onAction="#navigateToProductInventory" style="-fx-background-color: #520601;" text="Product Inventory" textFill="WHITE">
        <font>
            <Font size="15.0" />
        </font>
    </Button>
    <Button fx:id="orderHistoryButton" onAction="#navigateToOrderHistory" prefHeight="31.0" prefWidth="139.0" style="-fx-background-color: #520601;" text="Order History" textFill="WHITE">
        <font>
            <Font size="15.0" />
        </font>
    </Button>
</VBox>

<VBox fx:id="DisplayNew" prefHeight="400.0" prefWidth="660" spacing="10">
</VBox>

</HBox>